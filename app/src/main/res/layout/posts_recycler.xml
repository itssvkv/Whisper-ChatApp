<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:layout_marginVertical="10dp">    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/postsLayout"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="@drawable/posts_primary_container"        android:padding="5dp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">        <com.google.android.material.imageview.ShapeableImageView            android:id="@+id/currentUserIv"            android:layout_width="45dp"            android:layout_height="45dp"            android:layout_gravity="start"            android:layout_marginStart="10dp"            android:layout_marginTop="10dp"            android:scaleType="fitXY"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent"            app:shapeAppearanceOverlay="@style/userProfileImg"            tools:src="@drawable/app_icon" />        <TextView            android:id="@+id/currentUserName"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_marginStart="7dp"            android:layout_marginTop="12dp"            android:fontFamily="@font/poppins_semi_bold"            android:textColor="@color/black"            android:textSize="16sp"            app:layout_constraintStart_toEndOf="@id/currentUserIv"            app:layout_constraintTop_toTopOf="parent"            tools:text="Mohammed Gamal" />        <TextView            android:id="@+id/postCaption"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_marginHorizontal="10dp"            android:layout_marginTop="15dp"            android:fontFamily="@font/poppins_regular"            android:gravity="start"            android:padding="4dp"            android:textColor="@color/black"            android:textSize="14sp"            app:layout_constraintStart_toEndOf="@+id/currentUserIv"            app:layout_constraintTop_toBottomOf="@+id/currentUserName"            tools:text="Mohammed Gamal" />        <com.google.android.material.imageview.ShapeableImageView            android:id="@+id/postPhoto"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginHorizontal="10dp"            android:adjustViewBounds="true"            android:contentDescription="@string/post"            android:scaleType="fitXY"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/postCaption"            app:shapeAppearanceOverlay="@style/postsRoundedImg"            tools:src="@drawable/app_icon" />        <TextView            android:id="@+id/whoLikes"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginStart="10dp"            android:layout_marginTop="5dp"            android:fontFamily="@font/poppins_regular"            android:text="@string/postsLikesCount"            android:textColor="@color/black"            android:textSize="11sp"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintHorizontal_bias="0.0"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/postPhoto" />        <androidx.constraintlayout.widget.ConstraintLayout            android:id="@+id/likeLayout"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginTop="5dp"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintHorizontal_bias="1.0"            app:layout_constraintStart_toEndOf="@+id/whoLikes"            app:layout_constraintTop_toBottomOf="@id/postPhoto">            <ImageView                android:id="@+id/likeImg"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:contentDescription="@string/post"                android:src="@drawable/love"                android:layout_marginEnd="10dp"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintTop_toTopOf="parent" />            <TextView                android:id="@+id/likeNum"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:fontFamily="@font/poppins_semi_bold"                android:text="@string/numLikes"                android:textSize="14sp"                android:layout_marginEnd="4dp"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintEnd_toStartOf="@+id/likeImg"                app:layout_constraintTop_toTopOf="parent"                tools:text="10000" />        </androidx.constraintlayout.widget.ConstraintLayout>    </androidx.constraintlayout.widget.ConstraintLayout>    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/postsLayoutWithOutPhoto"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="@drawable/posts_primary_container"        android:padding="5dp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">        <com.google.android.material.imageview.ShapeableImageView            android:id="@+id/currentUserIvWithOutPhoto"            android:layout_width="45dp"            android:layout_height="45dp"            android:layout_gravity="start"            android:layout_marginStart="10dp"            android:layout_marginTop="10dp"            android:scaleType="fitXY"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent"            app:shapeAppearanceOverlay="@style/userProfileImg"            tools:src="@drawable/app_icon" />        <TextView            android:id="@+id/currentUserNameWithOutPhoto"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_marginStart="7dp"            android:layout_marginTop="12dp"            android:fontFamily="@font/poppins_semi_bold"            android:textColor="@color/black"            android:textSize="16sp"            app:layout_constraintStart_toEndOf="@id/currentUserIvWithOutPhoto"            app:layout_constraintTop_toTopOf="parent"            tools:text="Mohammed Gamal" />        <TextView            android:id="@+id/postCaptionWithOutPhoto"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_marginHorizontal="10dp"            android:layout_marginTop="15dp"            android:fontFamily="@font/poppins_regular"            android:gravity="start"            android:padding="4dp"            android:textColor="@color/black"            android:textSize="14sp"            app:layout_constraintStart_toEndOf="@+id/currentUserIvWithOutPhoto"            app:layout_constraintTop_toBottomOf="@+id/currentUserNameWithOutPhoto"            tools:text="Mohammed Gamal" />        <TextView            android:id="@+id/whoLikesWithOutPhoto"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginStart="10dp"            android:fontFamily="@font/poppins_regular"            android:text="@string/postsLikesCount"            android:textColor="@color/black"            android:textSize="11sp"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/postCaptionWithOutPhoto" />        <androidx.constraintlayout.widget.ConstraintLayout            android:id="@+id/likeLayoutWithOutPhoto"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintHorizontal_bias="1.0"            app:layout_constraintStart_toEndOf="@+id/whoLikesWithOutPhoto"            app:layout_constraintTop_toBottomOf="@id/postCaptionWithOutPhoto">            <ImageView                android:id="@+id/likeImgWithOutPhoto"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:contentDescription="@string/post"                android:src="@drawable/love"                android:layout_marginEnd="10dp"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintTop_toTopOf="parent" />            <TextView                android:id="@+id/likeNumWithOutPhoto"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:fontFamily="@font/poppins_semi_bold"                android:text="@string/numLikes"                android:textSize="14sp"                android:layout_marginEnd="4dp"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintEnd_toStartOf="@+id/likeImgWithOutPhoto"                app:layout_constraintTop_toTopOf="parent"                tools:text="10000" />        </androidx.constraintlayout.widget.ConstraintLayout>    </androidx.constraintlayout.widget.ConstraintLayout>    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/postsLayoutWithOutCaption"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="@drawable/posts_primary_container"        android:padding="5dp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">        <com.google.android.material.imageview.ShapeableImageView            android:id="@+id/currentUserIvWithOutCaption"            android:layout_width="45dp"            android:layout_height="45dp"            android:layout_gravity="start"            android:layout_marginStart="10dp"            android:layout_marginTop="10dp"            android:scaleType="fitXY"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent"            app:shapeAppearanceOverlay="@style/userProfileImg"            tools:src="@drawable/app_icon" />        <TextView            android:id="@+id/currentUserNameWithOutCaption"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_marginStart="7dp"            android:layout_marginTop="12dp"            android:fontFamily="@font/poppins_semi_bold"            android:textColor="@color/black"            android:textSize="16sp"            app:layout_constraintStart_toEndOf="@id/currentUserIvWithOutCaption"            app:layout_constraintTop_toTopOf="parent"            tools:text="Mohammed Gamal" />        <com.google.android.material.imageview.ShapeableImageView            android:id="@+id/postPhotoWithOutCaption"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginHorizontal="10dp"            android:layout_marginTop="20dp"            android:adjustViewBounds="true"            android:contentDescription="@string/post"            android:scaleType="fitXY"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/currentUserNameWithOutCaption"            app:shapeAppearanceOverlay="@style/postsRoundedImg"            tools:src="@drawable/app_icon" />        <TextView            android:id="@+id/whoLikesWithOutCaption"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginStart="10dp"            android:layout_marginTop="5dp"            android:fontFamily="@font/poppins_regular"            android:text="@string/postsLikesCount"            android:textColor="@color/black"            android:textSize="11sp"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/postPhotoWithOutCaption" />        <androidx.constraintlayout.widget.ConstraintLayout            android:id="@+id/likeLayoutWithOutCaption"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginTop="5dp"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintHorizontal_bias="1.0"            app:layout_constraintStart_toEndOf="@+id/whoLikesWithOutCaption"            app:layout_constraintTop_toBottomOf="@id/postPhotoWithOutCaption">            <ImageView                android:id="@+id/likeImgWithOutCaption"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:contentDescription="@string/post"                android:src="@drawable/love"                android:layout_marginEnd="10dp"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintTop_toTopOf="parent" />            <TextView                android:id="@+id/likeNumWithOutCaption"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginEnd="4dp"                android:fontFamily="@font/poppins_semi_bold"                android:text="@string/numLikes"                android:textSize="14sp"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintEnd_toStartOf="@+id/likeImgWithOutCaption"                app:layout_constraintTop_toTopOf="parent"                tools:text="10000" />        </androidx.constraintlayout.widget.ConstraintLayout>    </androidx.constraintlayout.widget.ConstraintLayout></androidx.constraintlayout.widget.ConstraintLayout>