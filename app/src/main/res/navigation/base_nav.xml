<?xml version="1.0" encoding="utf-8"?><navigation xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/base_nav"    app:startDestination="@id/splashFragment">    <fragment        android:id="@+id/splashFragment"        android:name="com.itssvkv.chatapp.ui.splash.SplashFragment"        android:label="fragment_splash"        tools:layout="@layout/fragment_splash">        <action            android:id="@+id/splashFragmentToOnBoardingFragment"            app:destination="@id/onBoardingFragment"            app:popUpTo="@id/splashFragment"            app:popUpToInclusive="true" />        <action            android:id="@+id/splashFragmentToAuthFragment"            app:destination="@id/sendOtpFragment"            app:popUpTo="@id/splashFragment"            app:popUpToInclusive="true" />        <action            android:id="@+id/splashFragmentToHomeFragment"            app:destination="@id/homeFragment"            app:popUpTo="@id/splashFragment"            app:popUpToInclusive="true" />    </fragment>    <fragment        android:id="@+id/onBoardingFragment"        android:name="com.itssvkv.chatapp.ui.onboarding.OnBoardingFragment"        android:label="fragment_on_boarding"        tools:layout="@layout/fragment_on_boarding">        <action            android:id="@+id/onBoardingFragmentToAuthFragment"            app:destination="@id/sendOtpFragment"            app:popUpTo="@id/onBoardingFragment"            app:popUpToInclusive="true" />    </fragment>    <fragment        android:id="@+id/sendOtpFragment"        android:name="com.itssvkv.chatapp.ui.auth.SendOtpFragment"        android:label="fragment_auth"        tools:layout="@layout/fragment_send_otp">        <action            android:id="@+id/sendFragmentToVerifyOtpFragment"            app:destination="@id/verifyOtpFragment"            app:popUpTo="@id/sendOtpFragment"            app:popUpToInclusive="true" />    </fragment>    <fragment        android:id="@+id/verifyOtpFragment"        android:name="com.itssvkv.chatapp.ui.auth.VerifyOtpFragment"        android:label="fragment_verify_otp"        tools:layout="@layout/fragment_verify_otp">        <action            android:id="@+id/verifyOtpFragmentToHomeFragment"            app:destination="@id/homeFragment"            app:popUpTo="@id/verifyOtpFragment"            app:popUpToInclusive="true" />    </fragment>    <fragment        android:id="@+id/homeFragment"        android:name="com.itssvkv.chatapp.ui.home.HomeFragment"        android:label="fragment_home"        tools:layout="@layout/fragment_home" >        <action            android:id="@+id/homeFragmentToRoomChatFragment"            app:destination="@id/roomChatFragment" />        <action            android:id="@+id/homeFragmentToUpdateFragment"            app:destination="@id/updateUserInfoFragment" />        <action            android:id="@+id/homeFragmentToCreatePostFragment"            app:destination="@id/createPostFragment" />    </fragment>    <fragment        android:id="@+id/roomChatFragment"        android:name="com.itssvkv.chatapp.ui.roomchat.RoomChatFragment"        android:label="fragment_room_chat"        tools:layout="@layout/fragment_room_chat" >        <argument            android:name="userInfo"            app:argType="com.itssvkv.chatapp.models.UserDataInfo"            app:nullable="true" />    </fragment>    <fragment        android:id="@+id/updateUserInfoFragment"        android:name="com.itssvkv.chatapp.ui.userprofile.UpdateUserInfoFragment"        android:label="fragment_update_user_info"        tools:layout="@layout/fragment_update_user_info" />    <fragment        android:id="@+id/createPostFragment"        android:name="com.itssvkv.chatapp.ui.createpost.CreatePostFragment"        android:label="fragment_create_post"        tools:layout="@layout/fragment_create_post" /></navigation>